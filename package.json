{
  "name": "plant-press-mern",
  "version": "1.0.0",
  "description": "Plant Press MERN stack project",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "server:debug": "nodemon --inspect app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kentzhang06/mern-stack-project.git"
  },
  "author": "Kent Z, Danny F, Jason K, Jon Z",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kentzhang06/mern-stack-project/issues"
  },
  "homepage": "https://github.com/kentzhang06/mern-stack-project#readme",
  "dependencies": {
    "aws-sdk": "^2.1005.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^6.3.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.10",
    "multer": "^1.4.3",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "react-icons": "^4.3.1",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.13"
  }
}
